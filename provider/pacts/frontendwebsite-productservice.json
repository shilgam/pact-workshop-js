{
  "consumer": {
    "name": "FrontendWebsite"
  },
  "provider": {
    "name": "ProductService"
  },
  "interactions": [
    {
      "description": "get all products",
      "providerState": "products exist",
      "request": {
        "method": "GET",
        "path": "/products"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "id": "09",
            "type": "CREDIT_CARD",
            "name": "Gem Visa"
          },
          {
            "id": "09",
            "type": "CREDIT_CARD",
            "name": "Gem Visa"
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 2
          },
          "$.body[*].*": {
            "match": "type"
          },
          "$.body[*].id": {
            "match": "type"
          },
          "$.body[*].type": {
            "match": "type"
          },
          "$.body[*].name": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "get product with ID 10",
      "providerState": "product with ID 10 exists",
      "request": {
        "method": "GET",
        "path": "/products/10"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "id": "10",
          "type": "CREDIT_CARD",
          "name": "28 Degrees"
        },
        "matchingRules": {
          "$.body.id": {
            "match": "type"
          },
          "$.body.type": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}